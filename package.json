{
  "name": "@dictadata/lib",
  "version": "1.0.3",
  "description": "dictadata library of common logger, utility and test modules",
  "author": "Drew O. Letcher",
  "license": "MIT",
  "keywords": [
    "dictadata"
  ],
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json",
    "./test": "./lib/test/index.js",
    "./test/*": "./lib/test/*.js",
    "./test/*.js": "./lib/test/*.js"
  },
  "bin": {
    "tr_launcher": "./lib/test/tr_launcher.js",
    "tr_http_queries": "./lib/test/tr_http_queries.js"
  },
  "scripts": {
    "lint": "eslint ./lib/**/*.js",
    "lint-fix": "eslint --fix ./lib/**/*.js",
    "test": "testrunner.bat"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/dictadata/js/lib.git"
  },
  "homepage": "https://gitlab.com/dictadata/js/lib#readme",
  "bugs": {
    "url": "https://gitlab.com/dictadata/js/lib/issues"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "eslint": "^9.1.0"
  },
  "dependencies": {
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
