{
  "name": "@dictadata/lib",
  "version": "1.0.0",
  "description": "dictadata library of common utility and test modules",
  "author": "Drew O. Letcher",
  "license": "MIT",
  "keywords": [
    "dictadata"
  ],
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./index.js",
  "exports": {
    ".": "./lib/index.js",
    "./lib": "./lib/index.js",
    "./lib/*": "./lib/*.js",
    "./lib/*.js": "./lib/*.js",
    "./utils": "./lib/utils/index.js",
    "./utils/*": "./lib/utils/*.js",
    "./utils/*.js": "./lib/utils/*.js",
    "./test": "./lib/test/index.js",
    "./test/*": "./lib/test/*.js",
    "./test/*.js": "./lib/test/*.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "tr_launcher": "lib/test/tr_launcher.js",
    "tr_http_client": "lib/test/tr_http_client.js"
  },
  "scripts": {
    "lint": "eslint lib/**/*.js",
    "lint-fix": "eslint --fix lib/**/*.js",
    "test": "testrunner.bat"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test",
    "example": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/dictadata/js/lib.git"
  },
  "homepage": "https://gitlab.com/dictadata/js/lib#readme",
  "bugs": {
    "url": "https://gitlab.com/dictadata/js/lib/issues"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "eslint": "^9.1.0"
  },
  "dependencies": {
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
